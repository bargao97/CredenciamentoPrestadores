<div class="box box-pane">
    <div class="box-header">
        <i class="fa fa-user"></i>
        <h3 class="box-title">Credenciamentos por região</h3>
        <div class="pull-right box-tools">
            <div class="btn-group">
                <button type="button" class="btn btn-success btn-sm modal-toggle" data-target="#GraficoOcorrenciaRegiao" data-toggle="modal">
                    <i class="fa fa-bars"></i>
                </button>
            </div>
            <button type="button" class="btn btn-success btn-sm" data-widget="collapse">
                <i class="fa fa-minus"></i>
            </button>
        </div>
    </div>
    <div class="box-body no-padding">
        <div id="GraficoCredenciamentosRegiao"></div>
    </div>
    <div class="box-footer text-black">
        <div class="row">
        </div>
    </div>
</div>
<div class="modal fade" id="GraficoOcorrenciaRegiao" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Credenciamento por regiao</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <h3>CONTRATO</h3>
                    @Html.DropDownList("IDCONTRATO", null, new { style = "width: 100%", @Class = "form-control" })
                    <br /><br />
                    <div class="row"><button class="btn btn-default" id="checkbtn" onclick="verificarGraficoregiao();" type="button" data-dismiss="modal"><span class="glyphicon glyphicon-check"></span> Verificar</button></div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
                </div>
            </div>
        </div>
    </div>
</div>