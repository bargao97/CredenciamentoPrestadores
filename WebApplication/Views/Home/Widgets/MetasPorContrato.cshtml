<div class="box box-pane">
    <div class="box-header">
        <i class="fa  fa-file-text-o"></i>
        <h3 class="box-title">Metas Por Contrato</h3>
        <div class="pull-right box-tools">
            <button type="button" class="btn btn-success btn-sm" data-widget="collapse">
                <i class="fa fa-minus"></i>
            </button>
        </div>
    </div>
    <div class="box-body no-padding">
        @Html.DropDownList("IDCONTRATOMETAS", null, "Selecione o contrato", new { style = "width: 100%", @Class = "form-control" })
        <select id="selectTipoMetas"name="selectTipoMetas"onchange = "MetasPorContrato(this.value);">
            <option selected="selected">Selecione o tipo de meta</option>
            <option value="1">Metas não concluidas</option>
            <option value="2">Metas Concluidas</option>
        </select> 
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>Especialidade</th>
                        <th>Meta</th>
                        <th>Concluido</th>
                        <th>Verificar Contratos Enviados</th>
                    </tr>
                </thead>
                <tbody id="metasContrato"></tbody>
            </table>
        </div>
    </div>
    <div class="box-footer text-black">
        <div class="row">
        </div>
    </div>
</div>
<div class="modal fade" id="ModalPrestAguar" role="dialog">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body">
                <div class="table-responsive"></div>
                <table class="table">
                    <thead>
                        <tr>
                            <th>PRESTADOR</th>
                            <th>ESPECIALIDADE</th>
                            <th>TELEFONE</th>
                            <th>CONTATO</th>
                            <th>DETALHES</th>
                        </tr>
                    </thead>
                    <tbody id="prestAguarCont">
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>